    "babel-preset-flow": "^6.23.0",
    "babel-preset-power-assert": "^1.0.0",
    "codecov": "^3.0.0",
    "cross-env": "^5.1.1",
    "flow-bin": "^0.59.0",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.3.0",
    "power-assert": "^1.4.4",
